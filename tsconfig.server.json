{
  "compilerOptions": {

"skipLibCheck": true,

    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "outDir": "dist",
    "types": ["node"]
  },
  "include": ["src", "payload-types.d.ts"],
  "exclude": ["node_modules"]
}
